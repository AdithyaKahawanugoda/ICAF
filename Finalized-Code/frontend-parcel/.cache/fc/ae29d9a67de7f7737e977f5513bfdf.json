olumn":15}}],"sources":{"src/screens/Research.js":"import React, { useState, useEffect } from \"react\";\nimport { Button, ListGroup } from \"react-bootstrap\";\nimport axios from \"axios\";\nimport \"regenerator-runtime/runtime\";\n\nconst Research = (props) => {\n  const rID = props.match.params.id;\n\n  const [resPaper, setResPaper] = useState([]);\n\n  useEffect(() => {\n    const getResarchPapers = async () => {\n      try {\n        await axios\n          .get(\n            \"http://localhost:6500/grid/api/researcherpvt/researchPapers\"\n          )\n          .then((res) => {\n            setResPaper(res.data.resPaper);\n            console.log(res.data.resPaper);\n          })\n          .catch((err) => {\n            alert(err.message);\n          });\n      } catch (err) {\n        alert(\"error: \" + err);\n      }\n    };\n    getResarchPapers();\n  }, []);\n  return (\n    <div>\n      <img\n        src=\"https://i.ibb.co/MRQ6577/Whats-App-Image-2021-06-27-at-3-51-27-PM.jpg\"\n        alt=\"research-resource-page-img\"\n        border=\"0\"\n        width=\"100%\"\n      />\n      {resPaper\n        .filter((wrk) => wrk._id === rID)\n        .map((research, index) => (\n          <div>\n            <ListGroup style={{ margin: \"5rem\" }}>\n              <ListGroup.Item>\n                <h3> {research.researchTopic}</h3>{\" \"}\n              </ListGroup.Item>\n              <ListGroup.Item>\n                Subject: {research.researchSubject}\n              </ListGroup.Item>\n              <ListGroup.Item>Authors: {research.paperAuthors}</ListGroup.Item>\n              <ListGroup.Item>\n                Abstract: <br></br> <p>{research.paperAbstract}</p>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Button variant=\"danger\" href={research.paperSecURL}>\n                  Download Research paper\n                </Button>{\" \"}\n              </ListGroup.Item>\n            </ListGroup>\n          </div>\n        ))}\n    </div>\n  );\n};\n\nexport default Research;\n"},"lineCount":null}},"error":null,"hash":"0b7557efa44c3abb595980c765d55b02","cacheData":{"env":{}}}