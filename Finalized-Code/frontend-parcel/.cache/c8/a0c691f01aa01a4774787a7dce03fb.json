llery.js","original":{"line":58,"column":51}},{"generated":{"line":121,"column":4},"source":"src/components/EditorGallery.js","original":{"line":58,"column":51}},{"generated":{"line":121,"column":11},"source":"src/components/EditorGallery.js","original":{"line":58,"column":58}},{"name":"addGalleryHandler","generated":{"line":121,"column":13},"source":"src/components/EditorGallery.js","original":{"line":58,"column":60}},{"generated":{"line":122,"column":0},"source":"src/components/EditorGallery.js","original":{"line":58,"column":14}},{"generated":{"line":122,"column":14},"source":"src/components/EditorGallery.js","original":{"line":47,"column":12}},{"generated":{"line":122,"column":15},"source":"src/components/EditorGallery.js","original":{"line":38,"column":10}},{"generated":{"line":122,"column":16},"source":"src/components/EditorGallery.js","original":{"line":37,"column":8}},{"generated":{"line":122,"column":17},"source":"src/components/EditorGallery.js","original":{"line":36,"column":6}},{"generated":{"line":122,"column":18},"source":"src/components/EditorGallery.js","original":{"line":34,"column":4}},{"generated":{"line":122,"column":19},"source":"src/components/EditorGallery.js","original":{"line":33,"column":2}},{"generated":{"line":123,"column":0},"source":"src/components/EditorGallery.js","original":{"line":67,"column":1}},{"generated":{"line":123,"column":1},"source":"src/components/EditorGallery.js","original":{"line":7,"column":0}},{"name":"EditorGallery","generated":{"line":125,"column":15},"source":"src/components/EditorGallery.js","original":{"line":69,"column":15}}],"sources":{"src/components/EditorGallery.js":"import React, { useState } from \"react\";\nimport { Form, Button, ListGroup } from \"react-bootstrap\";\nimport { Col, Row } from \"antd\";\nimport axios from \"axios\";\nimport FileBase from \"react-file-base64\";\n\nconst EditorGallery = () => {\n  const [dataStatus, setDataStatus] = useState(false);\n  const [fileEnc, setFileEnc] = useState(\"\");\n\n  const addGalleryHandler = async () => {\n    const config = {\n      headers: {\n        Authorization: `Bearer ${localStorage.getItem(\"authToken\")}`,\n      },\n    };\n    let postObject = { fileEnc };\n    await axios\n      .post(\n        \"http://localhost:6500/grid/api/editorpvt/addGallery\",\n        postObject,\n        config\n      )\n      .then((res) => {\n        alert(\"New Image added!\");\n        setDataStatus(!dataStatus);\n      })\n      .catch((err) => {\n        alert(\"ERROR! \" + err);\n      });\n  };\n\n  return (\n    <div>\n      <h1>Gallery</h1>\n      <div style={{ marginTop: \"4vh\" }}>\n        <Row style={{ marginTop: \"4vh\" }}>\n          <div\n            style={{\n              backgroundColor: \"#F1C40F\",\n              padding: \"1.5vh\",\n              borderRadius: \"8px\",\n              border: \"2px\",\n              width: \"25vw\",\n            }}\n          >\n            <Form.Group as={Col} md={12}>\n              <h4 style={{ marginBottom: \"2vh\" }}>Add Gallery Image</h4>\n              <div>\n                <FileBase\n                  type=\"file\"\n                  multiple={false}\n                  onDone={({ base64 }) => {\n                    setFileEnc(base64);\n                  }}\n                />\n              </div>\n              <Button style={{ marginTop: \"2vh\" }} onClick={addGalleryHandler}>\n                Upload\n              </Button>\n            </Form.Group>\n          </div>\n        </Row>\n      </div>\n    </div>\n  );\n};\n\nexport default EditorGallery;"},"lineCount":null}},"error":null,"hash":"3007640ab6608943c3c7f29b09a203e7","cacheData":{"env":{}}}