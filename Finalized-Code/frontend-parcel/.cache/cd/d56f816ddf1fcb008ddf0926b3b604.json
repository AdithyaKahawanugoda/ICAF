column":18},"source":"src/screens/ConferenceCover.js","original":{"line":56,"column":10}},{"generated":{"line":123,"column":85},"source":"src/screens/ConferenceCover.js","original":{"line":55,"column":8}},{"generated":{"line":123,"column":86},"source":"src/screens/ConferenceCover.js","original":{"line":31,"column":4}},{"generated":{"line":123,"column":87},"source":"src/screens/ConferenceCover.js","original":{"line":30,"column":2}},{"generated":{"line":124,"column":0},"source":"src/screens/ConferenceCover.js","original":{"line":61,"column":1}},{"generated":{"line":124,"column":1},"source":"src/screens/ConferenceCover.js","original":{"line":10,"column":0}},{"name":"ConferenceCover","generated":{"line":126,"column":15},"source":"src/screens/ConferenceCover.js","original":{"line":63,"column":15}}],"sources":{"src/screens/ConferenceCover.js":"import React, { useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport \"./ConferenceCover.css\";\nimport Cover from \"../components/Cover\";\nimport Shedule from \"../components/Shedule\";\nimport Profile from \"../components/Profile\";\nimport Workshop from \"../components/Workshop\";\nimport Reaserch from \"../components/Research\";\n\nconst ConferenceCover = () => {\n  const [conference, setConference] = useState(\"\");\n  useEffect(() => {\n    const getTimeLine = async () => {\n      try {\n        await axios\n          .get(\"http://localhost:6500/grid/api/guest/getConference\")\n          .then((res) => {\n            setConference(res.data.latestConference);\n          })\n          .catch((err) => {\n            alert(err.message);\n          });\n      } catch (err) {\n        alert(\"error :\" + err);\n      }\n    };\n    getTimeLine();\n  }, []);\n\n  return (\n    <div>\n      {conference.status === \"approvedbyadmin\" ? (\n        <div className=\"lkconference\">\n          <Cover conference={conference} />\n          <Shedule conference={conference} />\n          <Profile conference={conference} />\n          <div style={{ textAlign: \"center\" }}>\n            <h1>Workshops</h1>\n          </div>\n          {conference.addedWorkshops.map((val, key1) => (\n            <div key={key1}>\n              <Workshop id={val.workshopID} />\n            </div>\n          ))}\n          <div style={{ textAlign: \"center\" }}>\n            <h1>Research Papers</h1>\n          </div>\n          {conference.addedResearchPapers.map((val2, key2) => (\n            <div key={key2}>\n              <Reaserch id={val2.researchPaperID} />\n            </div>\n          ))}\n        </div>\n      ) : (\n        <div style={{ margin: \"50px\" }}>\n          <h2>No Conference Available</h2>\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default ConferenceCover;\n"},"lineCount":null}},"error":null,"hash":"e5b3b76d575b70464f0825b4f649ee63","cacheData":{"env":{}}}